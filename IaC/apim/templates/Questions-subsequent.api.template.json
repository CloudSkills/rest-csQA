{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "ApimServiceName": {
      "type": "string"
    }
  },
  "resources": [
    {
      "properties": {
        "apiVersion": "v1",
        "isCurrent": false,
        "apiVersionDescription": "version 1",
        "apiVersionSetId": "[resourceId('Microsoft.ApiManagement/service/apiVersionSets', parameters('ApimServiceName'), 'QuestionsApiVersionSet')]",
        "subscriptionRequired": false,
        "displayName": "Questions",
        "path": "questions",
        "protocols": [
          "http"
        ]
      },
      "name": "[concat(parameters('ApimServiceName'), '/Questions')]",
      "type": "Microsoft.ApiManagement/service/apis",
      "apiVersion": "2019-01-01",
      "dependsOn": []
    },
    {
      "properties": {},
      "name": "[concat(parameters('ApimServiceName'), '/starter/Questions')]",
      "type": "Microsoft.ApiManagement/service/products/apis",
      "apiVersion": "2019-01-01",
      "dependsOn": [
        "[resourceId('Microsoft.ApiManagement/service/apis', parameters('ApimServiceName'), 'Questions')]"
      ]
    }
  ]
}